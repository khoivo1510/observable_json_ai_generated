@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(Threads REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/UniversalObservableJsonTargets.cmake")

# Set component variables
set(UniversalObservableJson_VERSION @PROJECT_VERSION@)
set(UniversalObservableJson_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(UniversalObservableJson_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(UniversalObservableJson_VERSION_PATCH @PROJECT_VERSION_PATCH@)

# Backend information
set(UniversalObservableJson_BACKEND "@JSON_BACKEND_NAME@")
set(UniversalObservableJson_BACKEND_MACRO "@JSON_BACKEND_MACRO@")

check_required_components(UniversalObservableJson)
